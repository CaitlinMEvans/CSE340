<header id="top-header" role="banner">
  <span class="siteName">
    <a href="/" title="Return to home page" aria-label="CSE Motors homepage"
      >CSE Motors</a
    >
  </span>
  <div id="tools" role="navigation" aria-label="User account tools">
    <% if (locals.loggedin) { %>
    <a
      title="Click to manage account"
      href="/account/"
      aria-label="Access account management"
      >Welcome <%= accountData.account_firstname %></a
    >
    <a
      title="Click to log out"
      href="/account/logout"
      aria-label="Log out of your account"
      >Logout</a
    >
    <% } else { %>
    <a
      title="Click to log in"
      href="/account/login"
      aria-label="Access my account login page"
      >My Account</a
    >
    <% } %>
  </div>
</header>
